export fn foo (reg u64 x) {
reg bool co;
reg bool cf;
reg u64  sh;
(u32)[x + 1] = x;
(u32)[x + 0] = (u32)[x + 0] + 32s x;
(u32)[x + 0] +32s= x;
x +16u = 1;
sh     = 3;
co,cf,x = #x86_ROL(x,1);
co,cf,x = #x86_ROL(x,sh);
co,cf,x = #x86_ROR(x,1);
co,cf,x = #x86_ROR(x,sh);
co,cf,x = #x86_RCL(x,1,cf);
co,cf,x = #x86_RCL(x,sh,cf);
co,cf,x = #x86_RCR(x,1,cf);
co,cf,x = #x86_RCR(x,sh,cf);
x       = #x86_RORX(x,3);
}